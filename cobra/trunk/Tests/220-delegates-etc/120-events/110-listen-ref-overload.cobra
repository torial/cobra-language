class Control

	event click as EventHandler


class A

	def onClick(i as int)
		pass


class B
	inherits A

	cue init
		c = Control()
		listen c.click, ref .onClick

	def onClick(sender as Object, args as EventArgs)  # creates an overload due to method with the same name in an ancestor class
		pass


class Program

	def main is shared
		B()
