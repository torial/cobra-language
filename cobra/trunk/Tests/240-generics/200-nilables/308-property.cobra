# this tests the runtime checking of non-nil properties.
class Test

	var _foo as String

	def init
		_foo = ''

	pro foo as String
		get
			return _foo
		set
			_foo = value

	def bar
		expect ArgumentNullException
			s as String = nil to passthrough
			.foo = s

	shared
		def main
			t as Test = Test()
			t.bar()
