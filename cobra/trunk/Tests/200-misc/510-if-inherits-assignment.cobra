class One

	get foo as int
		return 1


class Two

	get bar as int
		return 2


class Test

	shared
		def main
			Test().run()

	def run
		.check(One(), 1)
		.check(Two(), 2)

	def check(obj as Object, value as int)
		if obj inherits One
			o as One = obj  # assign to a One variable with no casting
			assert o.foo==value
		else if obj inherits Two
			t as Two = obj  # assign to a Two variable with no casting
			assert t.bar==value
		else
			throw Exception('fall through')
