class Test

	shared
		def main
			assert 'foo' in 'foobar'
			assert 'foo' in 'barfoo'
			assert 'of' not in 'foobar'

			c as char = c'c'
			assert c in 'abc'

			Test().run()

	def run
		.doStuff(1, 2, 3)
		.doMoreStuff('a', 'b')

	def doStuff(values as vari int)
		assert 0 not in values
		assert 1 in values
		assert 2 in values
		assert 3 in values
		assert 4 not in values

	def doMoreStuff(values as vari String)
		assert 'a' in values
		assert 'b' in values
		assert 'c' not in values
		print 'done'
