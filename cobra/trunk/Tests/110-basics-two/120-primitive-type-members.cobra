class Test

	def main is shared
		c = c'A'
		assert c.isUpper
		assert not c.isLower
		c = c'a'
		assert not c.isUpper
		assert c.isLower
		c = c.toUpper
		assert c.isUpper
		assert not c.isLower

		d = 5.24
		assert d.negate == -5.24
		assert d.round == 5.0
		assert d.round(1) == 5.2

		d = decimal.parse('5.0')
		assert d == 5.0

		f = 3.5f
		assert f.sin.round(2) == -0.35f

		assert 5.getType.name.toLower.startsWith('int')

		assert 3.compareTo(5) < 0
