class Test

	def main is shared
		c = c'A'
		assert c.isUpper
		assert not c.isLower
		c = c'a'
		assert not c.isUpper
		assert c.isLower
		c = c.toUpper
		assert c.isUpper
		assert not c.isLower

		d = 5.0
		assert d.negate == -5.0
		assert d.round(1) == 5.0
		assert d.subtract(3.0) == 2.0

		d = decimal.parse('5.0')
		assert d == 5.0

		assert 5.getType.name.toLower.startsWith('int')

		assert 3.compareTo(5) < 0
