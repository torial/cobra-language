class Test

	var _items as int[]

	def init
		_items = @[1, 4, 9]

	pro [i as int] as int
		get
			return _items[i]
		set
			_items[i] = value

	pro [i as int, j as int] as int
		get
			return i + j

	get [key as String] as String
		return key

	def main
		is shared
		Test().check

	def check
		assert this[0]==1
		assert this[1]==4
		assert this[2]==9
		this[1] = 5
		assert this[1]==5
		this[2] = 6
		assert this[2]==6

		assert this[2, 4]==6
