
class Top

	cue init(i as int)
		pass

	def foo(i as int) as int
		return i

class Bottom
	inherits Top

	cue init(j as String, i as int)
		base.init(i)

	def foo(j as String, i as int) as int
		return base.foo(i) + j.length

	def main is shared
		b as Bottom = Bottom('aoeu', 0)
		assert b.foo("x2", 3)==5
