class MyObject

	def className as String  # make this a property
		return .getType().name


class Point
	inherits MyObject
	
	var _x as int
	var _y as int
	
	def init
		pass
		
	def init(x as int, y as int)
		_x = x
		_y = y

	def setXY(x as int, y as int)
		_x = x
		_y = y

	def main
		is shared
		p1 as Point = Point()
		p2 as Point = Point(3, 4)
		p1.setXY(1, 2)
		assert p1.className()=='Point'
		p2.setXY(1, 2)
