class A

	cue init
		pass
	
	cue init(x as int)
		pass
		
class B
	inherits A

	var _foo as int
	
	cue init
		_foo = 1
		base.init  # .error. can only be invoked

	cue init(x as int)
		_foo = 1
		base.init(1)  # .error. can only be invoked
		
	cue init(x as int, y as int)
		_foo = x
		.init  # .error. can only be invoked

	cue init(x as int, y as int, z as int)
		_foo = x
		.init(1)  # .error. can only be invoked
