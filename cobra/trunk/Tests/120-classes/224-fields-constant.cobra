# Test const - compile time constant
class AConst
	const YEAR as int = 1959
	const pcode = 'xyzzy'
	const _lastYear = .YEAR -1
	var _nyear 
	

	cue init
		assert AConst.YEAR == 1959
		assert .YEAR == 1959
		assert _lastYear == 1958
		assert .pcode == 'xyzzy'
		_nyear = AConst.YEAR +1
		
	def main is shared
		x = AConst()
		
		assert AConst.YEAR == 1959
		assert AConst._lastYear == 1958
		assert x._nyear == 1960

