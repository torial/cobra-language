# multiple levels of Nested class, infereed and explicit typing
class Enclosing
	
	class Nested is public

		def geti as int
			return 1

		class Nested2

			def geti as int
				return 2

	class NestedOther

		get geti
			return 10
			
	def geti as int 
		return 99
	
class Enc

	def main
		a = Enclosing()
		assert a.geti == 99

		en = Enclosing.Nested()
		assert en.geti == 1
	
		enn = Enclosing.Nested.Nested2()
		assert enn.geti == 2
		
		eno as Enclosing.NestedOther?
		eno = Enclosing.NestedOther()
		assert eno.geti == 10
