class Point

	var _x as int
	var _y as int

	cue init
		.init(0, 0)

	cue init(x as int, y as int)
		base.init
		_x = x
		_y = y

	def setXY(x as int, y as int)
		_x = x
		_y = y

	get sum as int
		return _x + _y

	def equals(x as int, y as int) as bool
		return _x==x and _y==y

	def _underscoredDoesNotRequireDot
		pass

	def test
		.setXY(1, 2)
		_underscoredDoesNotRequireDot
		assert .equals(1, 2)
		assert .sum==3

	def main
		is shared
		p1 as Point = Point()
		p1.test
