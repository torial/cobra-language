class Point

	var _x as int
	var _y as int

	def init
		pass

	def init(x as int, y as int)
		_x = x
		_y = y

	def setXY(x as int, y as int)
		_x = x
		_y = y

	get sum as int
		return _x + _y

	def equals(x as int, y as int) as bool
		return _x==x and _y==y

	def _underscoredDoesNotRequireDot
		pass

	def test()
		.setXY(1, 2)
		_underscoredDoesNotRequireDot()
		assert .equals(1, 2)
		assert .sum==3

	def main
		is shared
		p1 as Point = Point()
		p1.test()
