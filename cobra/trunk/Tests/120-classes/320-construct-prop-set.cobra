class X

	def main is shared
		s = Stuff(a=1, b=2)
		assert s.a == 1
		assert s.b == 2

		s = Stuff('a', b='b')
		assert s.a == 'a'
		assert s.b == 'b'

		letters = 'aoeu'
		s = Stuff(letters, b='b')
		assert s.a == letters
		assert s.b == 'b'


class Stuff

	var _a as dynamic?
	var _b as dynamic?
	
	def init
		pass

	def init(a as dynamic?)
		_a = a

	pro a from var
	
	pro b from var
