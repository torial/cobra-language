class Test

	def main is shared
		using file = File.createText('_correct-case.cobra')
			print to file
				print 'class X'
				print '    var _s as string'
				print '    def foo(o as object, s as string)'
				print '        _s = "aoeu"'
				print '        _s = s'
				print '        s = s.trim'
				print '        print s'
				print '    def main is shared'
				print '        print "hello"'
		process as System.Diagnostics.Process?
		output = CobraCore.runCobraExe('-correct-case _correct-case.cobra', out process)
		assert output.toLower.trim == 'hello'
		try, File.delete('correct-case.cobra')
		catch
			pass
