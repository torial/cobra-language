# test making a library internally specified with compiler directives
%% args -embed-run-time:no -target:lib -namespace:A.B

class Foo

	get one as int
		return 1

	get two as int
		return 2

	get name as String
		return .getType.name

	def computeString as String
		return 'aoeuaoeu'

	def checkInts(args as vari int) as int
		for arg in args
			assert arg
		return args.length
