		sw.write('if (CobraCore._willCheckAssert && !')
		_expr.writeSharpDef(sw)
		sw.write(') ')
		sw.indent
		sw.write('throw new AssertException([.sharpSourceSite], ')
		_expr.writeSharpBreakdown(sw)
		sw.write('[.sharpThis], ')
