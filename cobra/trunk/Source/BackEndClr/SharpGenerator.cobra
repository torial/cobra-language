class AttributeDecl
	is partial

	def writeSharpDef(sw as SharpWriter) is override
		.writeSharpDef(sw, '')

	def writeSharpDef(sw as SharpWriter, prefix as String)
		sw.write(r'[')
		sw.write(prefix)
		expr = _expr
		sw.write('[expr.name]')
		if expr.args.count
			sw.write('(')
			sep = ''
			for arg in expr.args
				sw.write(sep)
				if arg inherits AssignExpr
					sw.write(((arg.left to dynamic).name to String).capped) # CC: wouldn't need this if extension methods were registered with dynamic binder
					sw.write('=')
					arg = arg.right
				assert not arg inherits AssignExpr
				arg.writeSharpDefInContext(sw)
				sep = ', '
			sw.write(')')
		sw.writeLine(r']')


