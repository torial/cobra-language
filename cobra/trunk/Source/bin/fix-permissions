chmod $PERMS bin/clean
chmod $PERMS bin/fix-permissions
chmod $PERMS bin/install-from-workspace
chmod $PERMS bin/lgr
chmod $PERMS bin/make-snapshot
chmod $PERMS bin/make-trial
chmod $PERMS bin/testify
chmod $PERMS Snapshot/cobra
chmod $PERMS Snapshot/cobra.exe
chmod $PERMS cobra
