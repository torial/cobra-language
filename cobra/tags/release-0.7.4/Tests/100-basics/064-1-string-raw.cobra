class Test

	def main is shared

		s as String = ''

		s = r'aoeu'
		print s
		assert s.length==4
		assert s[3]==c'u'

		s = r'\'
		assert s.length==1
		assert s[0]==c'\\'

		s = r'\\'
		assert s.length==2
		assert s[0]==c'\\'
		assert s[1]==c'\\'

		s = r'['
		assert s.length==1
		assert s[0]==c'['

		s = r'[]'
		assert s.length==2
		assert s[0]==c'['
		assert s[1]==c']'
