namespace Test
	
	class Test
		
		def main
			is shared
				
			x as int = 0
			while x<10
				break
				x += 1
			assert x==0
			
			x = 1
			for i as int = 1 .. 10
				break
				x = i
			assert x==1
			
			s as String = 'a'
			for arg as String in Environment.getCommandLineArgs
				CobraCore.noOp(arg)
				break
				s = 'b'
			assert s=='a'
