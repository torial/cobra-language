#.require. jvm
namespace Test
	
	class Test
		
		def main
			is shared

			assert CobraCore.commandLineArgs[0].length
			assert CobraCore.commandLineArgs[0]<>'roembrjkxte,.nune,.ntokmbk'
			s as String = CobraCore.commandLineArgs[0]
			t as String = CobraCore.commandLineArgs[0]
			s = t
			assert s==t
			assert CobraCore.commandLineArgs.get(0) == s
