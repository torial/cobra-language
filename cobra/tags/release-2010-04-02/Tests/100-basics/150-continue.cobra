namespace Test
	
	class Test
		
		def main
			is shared
				
			count as int = 0
			x as int = 0
			while x<10
				x += 1
				continue
				count += 1
			assert x==10
			assert count==0
			
			x = 1
			for i as int = 1 .. 10
				x = i
				continue
				count += 1
			assert x==9
			assert count==0
			
			s as String = 'a'
			for arg as String in Environment.getCommandLineArgs
				CobraCore.noOp(arg)
				s = 'b'
				continue
				count += 1
			assert s=='b'
			assert count==0
