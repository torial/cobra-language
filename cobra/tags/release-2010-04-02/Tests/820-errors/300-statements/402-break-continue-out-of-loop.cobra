class Program

	def main
		i = 1
		continue # .error. continue
		for i in 2
			while true
				branch i
					on 1
						continue
					else
						i = 1
			while true
				branch i
					on 1, continue
					else, i = 1
		while true, break
		i = 1
		break # .error. break
		i = 1
		if true
			continue # .error. continue
		else
			break # .error. break
		