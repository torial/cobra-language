class Test

	def main is shared
		Test().run
	
	def run
		.testInt
		.testUInt16
		.testInt64
		
	def testInt
		i = 2
		j = 4
		k = i * j
		assert k == 8

	def testUInt16
		i = 2 to uint16
		j = 4 to uint16
		assert i < j
		assert j > i

	def testInt64
		i = 2 to int64
		j = 4 to int64
		i = 2
		j = 4
		k = i * j
		assert k == 8
		.passInt64(k)

	def passInt64(x as int64)
		assert x
